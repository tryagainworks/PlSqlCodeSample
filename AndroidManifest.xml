<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pcs.plsqlcodesample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pcs.plsqlcodesample.ActStartUp"
            android:label="@string/title_activity_act_start_up" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pcs.plsqlcodesample.ActTableConfig"
            android:label="@string/title_activity_act_table_config" >
        </activity>
        <activity
            android:name="com.pcs.plsqlcodesample.ActProcedureConfig"
            android:label="@string/title_activity_act_procedure_config" >
        </activity>
        <activity
            android:name="com.pcs.plsqlcodesample.ActFunctionConfig"
            android:label="@string/title_activity_act_function_config" >
        </activity>
        <activity
            android:name="com.pcs.plsqlcodesample.ActPackageConfig"
            android:label="@string/title_activity_act_package_config" >
        </activity>
        <activity
            android:name="com.pcs.plsqlcodesample.ActShowScript"
            android:label="@string/title_activity_act_show_script" >
        </activity>
    </application>

</manifest>
